{
  "name": "svelte-bootstrap-pug",
  "version": "2.0.0",
  "description": "Создание статических страниц с помощью технологий bootstrap, svelte, pug.",
  "main": "src/index.js",
  "scripts": {
    "compile": "rollup -c",
    "dev": "rollup -c -w",
    "clear": "rm -rf docs/index.html docs/build src/pug_modules/svg.pug src/pug_modules/settings.pug",
    "pug-svg-sprite": "node scripts/pug-svg-sprite.js",
    "pug-settings": "node scripts/pug-settings.js",
    "index.html": "npm run pug-svg-sprite && npm run pug-settings && node node_modules/pug-cli -o docs src/pug_modules/index.pug",
    "start": "sirv docs --no-clear"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yababay/svelte-bootstrap-pug.git"
  },
  "keywords": [
    "bootstrap",
    "svelte",
    "pug",
    "static",
    "svg",
    "html",
    "template"
  ],
  "author": "Михаил Беляков",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yababay/svelte-bootstrap-pug/issues"
  },
  "homepage": "https://github.com/yababay/svelte-bootstrap-pug#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.2.1",
    "sirv-cli": "^1.0.0",
    "jstransformer-markdown-it": "^2.1.0",
    "pug-cli": "^1.0.0-alpha6",
    "rollup": "^2.70.2",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sirv-cli": "^1.0.0",
    "svelte": "^3.0.0"
  },
  "dependencies": {
    "@yababay67/hash-router": "^0.0.8",
    "bootstrap-icons": "^1.8.1"
  }
}
