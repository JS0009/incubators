{
  "name": "with-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^13.5.0",
    "@yababay67/hash-router": "^0.1.7",
    "@yababay67/pug-modules": "^0.1.5",
    "jstransformer-markdown-it": "^2.1.0",
    "bootstrap-icons": "^1.8.2",
    "pug-cli": "^1.0.0-alpha6",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "pug-svg-sprite": "node scripts/pug-svg-sprite.js",
    "pug-settings": "node scripts/pug-settings.js",
    "index.pug": " node node_modules/@yababay67/pug-modules/index.js",
    "index.html": "npm run index.pug && npm run pug-svg-sprite && npm run pug-settings && node node_modules/pug-cli -o public src/pug_modules/index.pug",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "deploy": "rm -rf docs build && react-scripts build && mv build docs && sed 's/.\/static/\/incubators\/static/g ' -i docs/index.html  && git add . && git commit -am deploy && git push origin react",
    "serve": "serve -s docs",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
